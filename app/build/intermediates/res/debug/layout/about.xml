<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@color/blue">
    
    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@android:color/transparent"
        android:contentDescription="@string/backbutton"
        android:src="@drawable/back" />

    <TextView
        android:id="@+id/graphics"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/developer"
        android:layout_marginTop="40sp"
        android:text="@string/graphics"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TextView
        android:id="@+id/music"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/graphics"
        android:layout_marginTop="40sp"
        android:text="@string/music"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
        android:id="@+id/graphics0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/graphics"
        android:layout_alignParentLeft="true"
        android:text="@string/graphics_"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
        android:id="@+id/developer0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/graphics"
        android:layout_alignParentLeft="true"
        android:text="@string/developer_"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
        android:id="@+id/music0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/music"
        android:layout_alignParentLeft="true"
        android:text="@string/music_"
        android:textAppearance="?android:attr/textAppearanceMedium" />



    <TextView
        android:id="@+id/version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" />

    <TextView
        android:id="@+id/about_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:text="@string/about"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/developer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/about_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:text="@string/developer"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout><!-- From: file:/Users/royperdue/Documents/School_Work/Senior_Project/Gradebook-Backend/ANDROID_TEMPLATE_ANDROID_STUDIO/app/src/main/res/layout/about.xml -->